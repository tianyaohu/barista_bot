<?xml version="1.0"?>
<robot name="barista_robot">

  <material name="red">
      <color rgba="1.0 0.0 0.0 1"/>
  </material>

  <material name="green_light">
      <color rgba="0.0 1.0 0.0 1"/>
  </material>

  <material name="green_dark">
    <color rgba="0.0 0.5 0.0 1"/>
  </material>

  <material name="blue">
      <color rgba="0.0 0.0 1.0 1"/>
  </material>

  <material name="black">
      <color rgba="0.0 0.0 0.0 1"/>
  </material>

  <material name="white">
      <color rgba="1.0 1.0 1.0 1"/>
  </material>

  <material name="box_bot_blue">
      <color rgba="0.088656 0.428691 0.491021 1"/>
  </material>

  <link name="base_link">
  </link>


  <!-- Cylinder Base -->
  <link name="chassis">
    <visual>
      <geometry>
        <cylinder length="0.155" radius="0.178"/>
      </geometry>
      <material name="blue"/>
    </visual>
  </link>

  <joint name="base_link_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0" />
    <parent link="base_link" />
    <child link="chassis" />
  </joint>

  <!-- FL standoff rods -->
  <link name="standoff_rod_front_left">
    <visual>
      <geometry>
        <cylinder length="0.22" radius="0.01"/>
      </geometry>
      <material name="white"/>
    </visual>
  </link>

  <joint name="standoff_rod_FL_to_chassis_joint" type="fixed">
    <!-- <origin rpy="0 0 0" xyz="0.025 0.05 0.0775" /> -->
    <origin rpy="0 0 0" xyz="0.05 0.12 0.1875" />

    <parent link="chassis" />
    <child link="standoff_rod_front_left" />
  </joint>
  
  <!-- BL standoff rods -->
  <link name="standoff_rod_back_left">
    <visual>
      <geometry>
        <cylinder length="0.22" radius="0.01"/>
      </geometry>
      <material name="white"/>
    </visual>
  </link>

  <joint name="standoff_rod_BL_to_chassis_joint" type="fixed">
    <!-- <origin rpy="0 0 0" xyz="0.025 0.05 0.0775" /> -->
    <origin rpy="0 0 0" xyz="-0.05 0.12 0.1875" />

    <parent link="chassis" />
    <child link="standoff_rod_back_left" />
  </joint>


  <!-- FR standoff rods -->
  <link name="standoff_rod_front_right">
    <visual>
      <geometry>
        <cylinder length="0.22" radius="0.01"/>
      </geometry>
      <material name="white"/>
    </visual>
  </link>

  <joint name="standoff_rod_FR_to_chassis_joint" type="fixed">
    <!-- <origin rpy="0 0 0" xyz="0.025 0.05 0.0775" /> -->
    <origin rpy="0 0 0" xyz="0.05 -0.12 0.1875" />

    <parent link="chassis" />
    <child link="standoff_rod_front_right" />
  </joint>
  
  <!-- BR standoff rods -->
  <link name="standoff_rod_back_right">
    <visual>
      <geometry>
        <cylinder length="0.22" radius="0.01"/>
      </geometry>
      <material name="white"/>
    </visual>
  </link>

  <joint name="standoff_rod_BR_to_chassis_joint" type="fixed">
    <origin rpy="0 0 0" xyz="-0.05 -0.12 0.1875" />
    <parent link="chassis" />
    <child link="standoff_rod_back_right" />
  </joint>

  <!-- Cup Tray -->
   <link name="cup_tray">
    <visual>
      <geometry>
        <cylinder length="0.09" radius="0.178"/>
      </geometry>
      <material name="blue"/>
    </visual>
  </link>

  <joint name="cup_tray_to_chassis_joint" type="fixed">
    <origin rpy="0 0 0" xyz="0 0 0.3425" />
    <parent link="chassis" />
    <child link="cup_tray" />
  </joint>

  <!-- Motorized Left WHeel -->
   <link name="left_wheel">      
      <visual>
        <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
        <geometry>
          <cylinder length="0.0206" radius="0.0352"/>
        </geometry>
        <material name="black"/>
      </visual>
  </link>

  <joint name="joint_left_wheel" type="continuous">
    <origin rpy="0 0 0" xyz="0 0.128 -0.0775"/>
    <child link="left_wheel"/>
    <parent link="chassis"/>
    <axis rpy="0 0 0" xyz="0 1 0"/>
    <limit effort="10000" velocity="1000"/>
    <joint_properties damping="1.0" friction="1.0"/>
  </joint>


  <!-- Motorized Right WHeel -->
   <link name="right_wheel">      
      <visual>
        <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
        <geometry>
          <cylinder length="0.0206" radius="0.0352"/>
        </geometry>
        <material name="black"/>
      </visual>
  </link>

  <joint name="joint_right_wheel" type="continuous">
    <origin rpy="0 0 0" xyz="0 -0.128 -0.0775"/>
    <child link="right_wheel"/>
    <parent link="chassis"/>
    <axis rpy="0 0 0" xyz="0 1 0"/>
    <limit effort="10000" velocity="1000"/>
    <joint_properties damping="1.0" friction="1.0"/>
  </joint>

  <!-- Caster Front Wheel -->
   <!-- yaw -->
  <link name="front_yaw_link">
    <visual>
        <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
        <geometry>
          <cylinder length="0.005" radius="0.0352"/>
        </geometry>
        <material name="blue"/>
      </visual>
  </link>

  <joint name="front_yaw_joint" type="continuous">
    <origin rpy="0 0 0" xyz="0.12 0 -0.0775" />
    <parent link="chassis" />
    <child link="front_yaw_link" />
    <axis xyz="0 0 1" />
    <limit effort="1000.0" velocity="100.0" />
    <dynamics damping="0.0" friction="0.1"/>
  </joint>

<!-- roll -->
  <link name="front_roll_link">
      <visual>
        <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
        <geometry>
          <cylinder length="0.005" radius="0.0352"/>
        </geometry>
        <material name="red"/>
      </visual>
  </link>

  <joint name="front_roll_joint" type="continuous">
    <origin rpy="0 0 0" xyz="0 0 0" />
    <parent link="front_yaw_link" />
    <child link="front_roll_link" />
    <axis xyz="1 0 0" />
    <limit effort="1000.0" velocity="100.0" />
    <dynamics damping="0.0" friction="0.1"/>
  </joint>

<!-- pitch -->
  <link name="front_pitch_link">
    <visual>
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
      <geometry>
        <sphere radius="0.0352"/>
      </geometry>
      <material name="green_dark"/>
    </visual>
  </link>

  <joint name="front_pitch_joint" type="continuous">
    <origin rpy="0 0 0" xyz="0 0 0" />
    <parent link="front_roll_link" />
    <child link="front_pitch_link" />
    <axis xyz="0 1 0" />
    <limit effort="1000.0" velocity="100.0" />
    <dynamics damping="0.0" friction="0.1"/>
  </joint>


<!-- Caster Wheel Back -->
  <link name="back_yaw_link">
    <visual>
        <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
        <geometry>
          <cylinder length="0.005" radius="0.0352"/>
        </geometry>
        <material name="blue"/>
      </visual>
  </link>

  <joint name="back_yaw_joint" type="continuous">
    <origin rpy="0 0 0" xyz="-0.12 0 -0.0775" />
    <parent link="chassis" />
    <child link="back_yaw_link" />
    <axis xyz="0 0 1" />
    <limit effort="1000.0" velocity="100.0" />
    <dynamics damping="0.0" friction="0.1"/>
  </joint>



  <link name="back_roll_link">
      <visual>
        <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
        <geometry>
          <cylinder length="0.005" radius="0.0352"/>
        </geometry>
        <material name="red"/>
      </visual>
  </link>

  <joint name="back_roll_joint" type="continuous">
    <origin rpy="0 0 0" xyz="0 0 0" />
    <parent link="back_yaw_link" />
    <child link="back_roll_link" />
    <axis xyz="1 0 0" />
    <limit effort="1000.0" velocity="100.0" />
    <dynamics damping="0.0" friction="0.1"/>
  </joint>



  <link name="back_pitch_link">
    <visual>
      <origin rpy="0 1.5707 1.5707" xyz="0 0 0"/>
      <geometry>
        <sphere radius="0.0352"/>
      </geometry>
      <material name="green_light"/>
    </visual>
  </link>

  <joint name="back_pitch_joint" type="continuous">
    <origin rpy="0 0 0" xyz="0 0 0" />
    <parent link="back_roll_link" />
    <child link="back_pitch_link" />
    <axis xyz="0 1 0" />
    <limit effort="1000.0" velocity="100.0" />
    <dynamics damping="0.0" friction="0.1"/>
  </joint>



</robot>